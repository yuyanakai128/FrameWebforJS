{
    "name": "frame-web-for-js",
    "version": "2.0.10",
    "scripts": {
        "ng": "ng",
        "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
        "build": "ng build --configuration production --base-href \"https://structuralengine.github.io/FrameWebforJS/\" && angular-cli-ghpages",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:dev": "concurrently \"ng build --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
        "start:dev": "electron dist/shell.js",
        "electron:windows": "npm run build:prod && npx electron-builder build --windows",
        "build:prod": "ng build --configuration production --optimization --outputHashing=all --sourceMap=false --namedChunks=false --extract-licenses --vendorChunk=false --build-optimizer && webpack --config webpack.electron.prod.config.js && npm run filecopy1 && npm run filecopy2",
        "start:prod": "electron dist/index.js",
        "filecopy1": "node -e \"require('fs').copyFileSync('dev-app-update.yml', 'dist/dev-app-update.yml')\"",
        "filecopy2": "node -e \"require('fs').copyFileSync('app-update.yml', 'dist/app-update.yml')\""
    },
    "description": "FrameWebforJS",
    "author": {
        "name": "MALME INC.",
        "email": "info@malme.co.jp"
    },
    "keywords": [
        "angular",
        "angular 6",
        "typescript",
        "sass"
    ],
    "private": true,
    "dependencies": {
        "@angular-devkit/schematics": "^12.2.7",
        "@angular/animations": "~12.2.7",
        "@angular/cdk": "~12.2.0",
        "@angular/common": "~12.2.7",
        "@angular/compiler": "~12.2.7",
        "@angular/core": "~12.2.7",
        "@angular/fire": "^6.1.4",
        "@angular/forms": "~12.2.7",
        "@angular/localize": "^12.2.7",
        "@angular/material": "~12.2.0",
        "@angular/platform-browser": "~12.2.7",
        "@angular/platform-browser-dynamic": "~12.2.7",
        "@angular/router": "~12.2.7",
        "@ng-bootstrap/ng-bootstrap": "^10.0.0",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "@types/file-saver": "^2.0.1",
        "@types/jquery": "3.3.29",
        "@types/jqueryui": "1.12.7",
        "angular-cli-ghpages": "^0.6.0",
        "bootstrap": "^4.4.0",
        "electron-is-dev": "^2.0.0",
        "electron-nsis": "0.0.1-alpha",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "^4.6.5",
        "file-saver": "^2.0.2",
        "firebase": "^8.3.1",
        "fontisto": "^3.0.4",
        "html2canvas": "^1.1.3",
        "jquery": "^3.6.0",
        "jquery-ui-pack": "1.12.3",
        "jszip": "^2.5.0",
        "pako": "^2.0.2",
        "pqgrid": "file:./paramquery-8.4.0/",
        "print-js": "^1.6.0",
        "rxjs": "~6.6.2",
        "rxjs-compat": "^6.5.3",
        "three": "^0.128.0",
        "troika-three-text": "^0.42.0",
        "ts-loader": "^9.2.5",
        "tslib": "^2.0.0",
        "update-electron-app": "^2.0.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.2.7",
        "@angular/cli": "^12.2.7",
        "@angular/compiler-cli": "~12.2.7",
        "@angular/language-service": "~12.2.7",
        "@fortawesome/fontawesome-free": "^5.14.0",
        "@types/jasmine": "~3.8.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^15.6.0",
        "@types/three": "^0.128.0",
        "bootstrap": "^4.3.1",
        "codelyzer": "^6.0.0",
        "concurrently": "^6.2.1",
        "electron": "^13.2.0",
        "electron-builder": "^22.14.13",
        "electron-log": "4.4.1",
        "electron-packager": "^15.3.0",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ngx-electron": "^2.2.0",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.3.5",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    }
}
